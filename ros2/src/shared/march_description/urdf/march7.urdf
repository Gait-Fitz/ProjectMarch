<?xml version="1.0" ?>
<robot name="test_robot">
    <link name="left_foot">
        <visual>
            <origin xyz="0 0.25 0" rpy="0 1.57079632679 0"/>
            <geometry>
                <cylinder length="0.3" radius="0.05"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0.25 0" rpy="0 1.57079632679 0"/>
            <geometry>
                <cylinder length="0.3" radius="0.05"/>
            </geometry>
        </collision>
    </link>

    <link name="left_lower_leg">
        <visual>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
        </collision>
    </link>

    <link name="left_upper_leg">
        <visual>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
        </collision>
    </link>

    <link name="hip">
        <visual>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
            <material name="Green">
                <color rgba="0 1 0 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
        </collision>
    </link>

    <link name="right_upper_leg">
        <visual>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
        </collision>
    </link>

    <link name="right_lower_leg">
        <visual>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
        </collision>
    </link>

    <link name="right_foot" type="floating">
        <visual>
            <origin xyz="0.15 0 0" rpy="0 1.57079632679 0"/>
            <geometry>
                <cylinder length="0.3" radius="0.05"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.15 0 0" rpy="0 1.57079632679 0"/>
            <geometry>
                <cylinder length="0.3" radius="0.05"/>
            </geometry>
        </collision>
    </link>


    <joint name="left_ankle" type="revolute">
        <parent link="left_foot"/>
        <child link="left_lower_leg"/>
        <origin xyz="-0.15 0.25 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit effort="20" lower="-0.75" upper="0.5" velocity="1" />
    </joint>

    <joint name="left_knee" type="revolute">
        <parent link="left_lower_leg"/>
        <child link="left_upper_leg"/>
        <origin xyz="0 0 0.5" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit effort="20" lower="-1.05" upper="0" velocity="1" />
    </joint>

    <joint name="left_hip" type="revolute">
        <parent link="left_upper_leg"/>
        <child link="hip"/>
        <origin xyz="0 0.0 0.5" rpy="1.57079632679 0 0" />
        <axis xyz="0 0 1" />
        <limit effort="20" lower="-0.5" upper="0.5" velocity="1" />
    </joint>

    <joint name="right_hip" type="revolute">
        <parent link="hip"/>
        <child link="right_upper_leg"/>
        <origin xyz="0 0 0.5" rpy="1.57079632679 0 0" />
        <axis xyz="0 1 0" />
        <limit effort="20" lower="-0.5" upper="0.5" velocity="1" />
    </joint>

    <joint name="right_knee" type="revolute">
        <parent link="right_upper_leg"/>
        <child link="right_lower_leg"/>
        <origin xyz="0 0 0.5" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit effort="20" lower="-1.05" upper="0" velocity="1" />
    </joint>

    <joint name="right_ankle" type="revolute">
        <parent link="right_lower_leg"/>
        <child link="right_foot"/>
        <origin xyz="0 0 0.5" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit effort="20" lower="-0.75" upper="0.5" velocity="1" />
    </joint>

    <ros2_control name="FakeSystem" type="system">
        <hardware>
            <plugin>fake_components/GenericSystem</plugin>
        </hardware>
        <joint name="left_ankle">
            <param name="initial_position">0.0</param>
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="left_knee">
            <param name="initial_position">0.0</param>
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="left_hip">
            <param name="initial_position">0.0</param>
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="right_hip">
            <param name="initial_position">0.0</param>
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="right_knee">
            <param name="initial_position">0.0</param>
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="right_ankle">
            <param name="initial_position">0.0</param>
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
    </ros2_control>

</robot>