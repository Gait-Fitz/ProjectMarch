<launch>

    <param name="robot_description"
           command="$(find xacro)/xacro --inorder '$(find march_description)/urdf/march-iv.xacro'" />

    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>

    <rosparam file="$(find march_rqt_gait_generator)/config/layout.yaml" command="load"/>

    <node name="march_rqt_gait_generator" pkg="march_rqt_gait_generator" type="march_rqt_gait_generator" output="screen"/>
</launch>
