<launch>
    <node name="talker_0"
          pkg="march_main" type="master_node">
        <remap from="master/gait_input" to="~output" />
    </node>

    <node name="talker_1"
          pkg="march_main" type="gait_node">
    </node>

    <test name="publishtest"
          test-name="master_node_publish_test"
          pkg="march_main"
          type="publishtest"
          time-limit="10.0">
        <rosparam>
            topics:
              - name: talker_0/output
                timeout: 3
                negative: False
              - name: talker_1/output
                timeout: 3
                negative: True  # means it is not published until the timeout
        </rosparam>
    </test>
</launch>